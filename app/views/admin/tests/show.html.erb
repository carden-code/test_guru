<p>
  <strong>Title:</strong>
  <%= @test.title %>
</p>

<p>
  <strong>Level:</strong>
  <%= test_level(@test) %>
</p>

<ul>
  <% @test.questions.each do |question| %>
    <li>
      <%= question.body %>
      <%= link_to 'Check', admin_question_path(question) %>
      <%= link_to 'Delete', admin_question_path(question), method: :delete, data: { confirm: 'Are you sure?' } %>
      <%= link_to 'Edit', edit_admin_question_path(question) %>
    </li>
  <% end %>
</ul>

<%= link_to 'New queston', new_admin_test_question_path(@test) %><br />
<%= link_to 'Edit', edit_admin_test_path(@test) %>
<%= link_to 'Back', admin_tests_path %>

